-- A largish example to solve sudoku puzzles

import foundation;

type cons<%t> is nil or cons{%t,cons<%t>};

function length(L:cons<%t>):#int =>
  switch L in {
    nil -> 0;
    cons{_:%t,Tl:cons<%t>} -> let{
      var Tlen:#int is length(Tl)
    } in Tlen+1;
  };

procedure main(){
  __#outStr(__#logFile,"sudoku\n");
}
  